{
  "name": "stereo-api",
  "type": "module",
  "version": "0.0.0",
  "description": "API backend for Stereo",
  "repository": "https://github.com/hoangvvo/stereo-api.git",
  "author": "Hoang Vo (https://hoangvvo.com/)",
  "license": "AGPL-3.0",
  "scripts": {
    "start": "node dist/main.js",
    "clean": "npx rimraf dist",
    "build:ts": "tsc",
    "build:copy": "npx cpy ./*/*.graphql --parents --cwd=src/graphql ../../dist/graphql",
    "build": "npm run codegen && npm run build:ts && npm run build:copy",
    "dev": "npx concurrently \"npm run codegen:watch\" \"npm run dev:watch\"",
    "dev:watch": "nodemon --ext ts --watch src --exec \"node -r dotenv/config --loader ts-node/esm/transpile-only\" ./src/main.ts",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch",
    "lint": "eslint ./ --ext json,ts,tsx --ignore-path .gitignore"
  },
  "dependencies": {
    "@benzene/core": "^0.8.0",
    "@benzene/extra": "^0.2.1",
    "@benzene/http": "^0.4.0",
    "@benzene/jit": "^0.1.3",
    "@benzene/ws": "^0.7.0",
    "@graphql-tools/load-files": "^6.3.2",
    "@graphql-tools/merge": "^8.0.2",
    "@graphql-tools/schema": "^8.1.2",
    "@sentry/node": "^6.11.0",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "fast-json-stringify": "^2.7.8",
    "googleapis": "^84.0.0",
    "graphql": "^15.5.1",
    "graphql-scalars": "^1.10.0",
    "ioredis": "^4.27.8",
    "mongodb": "^4.1.0",
    "nanoid": "^3.1.25",
    "next-connect": "^0.10.2",
    "slug": "^5.1.0",
    "undecim": "^0.1.1",
    "ws": "^8.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.1.1",
    "@graphql-codegen/typescript": "^2.1.0",
    "@graphql-codegen/typescript-resolvers": "^2.1.0",
    "@types/cors": "^2.8.12",
    "@types/ioredis": "^4.26.7",
    "@types/node": "^16.7.1",
    "@types/slug": "^5.0.2",
    "@types/spotify-api": "^0.0.11",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "concurrently": "^6.2.1",
    "cpy-cli": "^3.1.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "16.x"
  },
  "private": true
}
