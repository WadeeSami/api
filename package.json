{
  "name": "stereo-api",
  "version": "0.0.0",
  "description": "API backend for Stereo",
  "repository": "https://github.com/hoangvvo/stereo-api.git",
  "author": "Hoang Vo (https://hoangvvo.com/)",
  "license": "AGPL-3.0",
  "scripts": {
    "start": "node dist/server",
    "clean": "npx rimraf dist",
    "build:ts": "tsc",
    "build:copy": "npx cpy ./*/*.graphql --parents --cwd=src/graphql ../../dist/graphql",
    "build": "npm run codegen && npm run build:ts && npm run build:copy",
    "dev": "tsnd -r dotenv/config --respawn --transpile-only src/server",
    "codegen": "graphql-codegen",
    "lint": "eslint ./ --ext js,ts,tsx --ignore-path .gitignore"
  },
  "dependencies": {
    "@benzene/server": "^0.2.0",
    "@benzene/ws": "^0.3.0",
    "@graphql-tools/load-files": "^6.2.5",
    "@graphql-tools/merge": "^6.2.6",
    "@graphql-tools/schema": "^7.1.2",
    "@sentry/node": "^5.29.2",
    "cloudinary": "^1.23.0",
    "connect-redis": "^5.0.0",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "fast-json-stringify": "^2.3.0",
    "googleapis": "^66.0.0",
    "graphql": "^15.4.0",
    "graphql-scalars": "^1.7.0",
    "graphql-upload": "^11.0.0",
    "ioredis": "^4.19.4",
    "mongodb": "^3.6.3",
    "nanoid": "^3.1.20",
    "next-connect": "^0.9.1",
    "next-session": "^3.4.0",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-spotify": "^1.1.0",
    "slug": "^4.0.2",
    "ws": "^7.4.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-resolvers": "^1.18.1",
    "@types/connect-redis": "^0.0.16",
    "@types/cors": "^2.8.9",
    "@types/ioredis": "^4.17.10",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.19",
    "@types/node-fetch": "^2.5.7",
    "@types/passport": "^1.0.5",
    "@types/pino": "^6.3.4",
    "@types/slug": "^0.9.1",
    "@types/spotify-api": "^0.0.6",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "cpy-cli": "^3.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "prettier": "^2.2.1",
    "ts-node-dev": "1.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": "^14.0.0"
  },
  "private": true
}
